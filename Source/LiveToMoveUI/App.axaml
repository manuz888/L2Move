<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="LiveToMoveUI.App"
             xmlns:local="using:LiveToMoveUI"
             RequestedThemeVariant="Dark">
    
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
    </Application.Styles>
    
    <Application.Resources>
        <FontFamily x:Key="SilkscreenFont">avares://LiveToMoveUI/Assets/Silkscreen-Regular.ttf#Silkscreen</FontFamily>
        <FontFamily x:Key="PressStartFont">avares://LiveToMoveUI/Assets/PressStart2P-Regular.ttf#</FontFamily>
        <FontFamily x:Key="MunroFont">avares://LiveToMoveUI/Assets/Munro.ttf#</FontFamily>
    </Application.Resources>
</Application>